.bill-list{
  padding:25rpx 30rpx;
  font-size: 30rpx;
  color:#AAA;
  border-bottom: 1px solid #DDD;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.bill-list text{
  display:block;
}
.bill-list .amount{
  color: red;
  font-size: 35rpx;
  font-weight: bold;
  text-align: center;
}
